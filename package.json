{
  "name": "hana_capm",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/approuter": "^20.8.2",
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.0.1",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "build": "cds build",
    "watch-interaction": "cds watch --open interaction/index.html?sap-ui-xx-viewCache=false --livereload false",
    "approuter":"cds bind --exec -- node node_modules/@sap/approuter/approuter.js --workingDir app/router"
  },
  "private": true,
  "cds": {
    "requires": {
      "db": "xsuaa",
      "auth": "mocked",
      "[production]": {
        "auth": "xsuaa"
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/interaction"
  ]
}
